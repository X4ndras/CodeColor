{
  "name": "neovim-theme-creator",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json && tsc -p tsconfig.node.json",
    "prepare": "npm run smui-theme-light && npm run smui-theme-dark",
    "smui-theme-light": "smui-theme compile public/smui-light.css -i src/theme",
    "smui-theme-dark": "smui-theme compile public/smui-dark.css -i src/theme/dark",
    "postinstall": "mkdir -p public/tree-sitter && cp node_modules/web-tree-sitter/tree-sitter.wasm public/tree-sitter/ && node scripts/copy-wasm.js"
  },
  "dependencies": {
    "@smui/button": "^8.0.0-beta.3",
    "@smui/card": "^8.0.0-beta.3",
    "@smui/switch": "^8.0.0-beta.3",
    "highlight.js": "^11.11.1",
    "svelte-material-ui": "^8.0.0-beta.3",
    "tinycolor2": "^1.6.0",
    "web-tree-sitter": "^0.25.3"
  },
  "devDependencies": {
    "@mdi/js": "^7.4.47",
    "@smui/dialog": "^8.0.0-beta.3",
    "@smui/snackbar": "^8.0.0-beta.3",
    "@smui/tab": "^8.0.0-beta.3",
    "@smui/tab-bar": "^8.0.0-beta.3",
    "@smui/textfield": "^8.0.0-beta.3",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@tsconfig/svelte": "^5.0.4",
    "@types/tinycolor2": "^1.4.6",
    "smui-theme": "^8.0.0-beta.3",
    "svelte": "^5.20.4",
    "svelte-check": "^4.1.4",
    "tree-sitter-c": "^0.23.5",
    "tree-sitter-cli": "^0.25.3",
    "tree-sitter-css": "^0.23.2",
    "tree-sitter-go": "^0.23.4",
    "tree-sitter-javascript": "^0.23.1",
    "tree-sitter-lua": "^2.1.3",
    "tree-sitter-python": "^0.23.6",
    "tree-sitter-rust": "^0.23.2",
    "tree-sitter-typescript": "^0.23.2",
    "tslib": "^2.8.1",
    "typescript": "^5.7.3",
    "vite": "^6.2.0"
  }
}
